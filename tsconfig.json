{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es2015",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types"],
  },
  "include": ["src/**/*"],
  "exclude": ["functions/**/*"]
}
